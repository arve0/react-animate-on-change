{
  "name": "react-animate-on-change",
  "version": "1.0.1",
  "description": "Animate your components on state change",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js -d dist",
    "example": "concurrent \"ecstatic ./example\" \"npm run ewebpack\"",
    "ewebpack": "webpack --config example/webpack.config.js --watch",
    "postversion": "npm run build && git add dist && git commit --amend --no-edit",
    "postpublish": "git push && git push --tags",
    "start": "karma start",
    "test": "karma start karma.conf-ci.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arve0/react-animate-on-change.git"
  },
  "keywords": [
    "react-component",
    "react",
    "animate",
    "animation",
    "css"
  ],
  "author": "Arve Seljebu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arve0/react-animate-on-change/issues"
  },
  "homepage": "https://github.com/arve0/react-animate-on-change#readme",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "concurrently": "^1.0.0",
    "core-js": "^2.5.1",
    "css-loader": "^0.23.1",
    "ecstatic": "^1.4.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sauce-launcher": "^1.1.0",
    "prop-types": "^15.5.10",
    "react": "^0.14.5 || ^15.0.0 || ^16.0.0",
    "react-dom": "^0.14.5 || ^15.0.0 || ^16.0.0",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "style-loader": "^0.13.0",
    "watchify": "^3.9.0",
    "webpack": "^1.12.11"
  },
  "peerDependencies": {
    "react": "^0.14.5 || ^15.0.0 || ^16.0.0",
    "prop-types": "^15.5.10"
  }
